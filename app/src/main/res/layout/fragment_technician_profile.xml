<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".technician.fragments.TechnicianProfileFragment">

    <!-- TODO: Update blank fragment layout -->
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="@dimen/forty"
            android:fontFamily="@font/sans_bold"
            android:gravity="center_vertical"
            android:paddingLeft="@dimen/ten"
            android:text="Technician Profile"
            android:textColor="@color/black"
            android:textSize="@dimen/seventeen" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <androidx.core.widget.NestedScrollView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:scrollbars="none">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:orientation="vertical">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <androidx.cardview.widget.CardView
                            android:layout_width="@dimen/hundred_eighty"
                            android:layout_height="@dimen/hundred_eighty"
                            android:layout_centerInParent="true"
                            android:layout_marginTop="@dimen/thirty"
                            android:layout_marginBottom="@dimen/thirty"
                            app:cardCornerRadius="@dimen/ninety">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">

                                <ImageView
                                    android:id="@+id/profileimage"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:scaleType="centerCrop"
                                    android:src="@drawable/dummy_profilepic" />

                            </LinearLayout>

                        </androidx.cardview.widget.CardView>

                        <ImageView
                            android:id="@+id/logoutbtn"
                            android:layout_width="@dimen/fifty"
                            android:layout_height="@dimen/fifty"
                            android:padding="@dimen/eleven"
                            android:layout_alignParentRight="true"
                            android:layout_margin="@dimen/fifteen"
                            android:src="@drawable/logout_btn"/>

                    </RelativeLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:paddingLeft="@dimen/fifteen"
                        android:paddingRight="@dimen/fifteen">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/name"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/fifty"
                                android:layout_marginTop="@dimen/ten"
                                android:background="@drawable/box"
                                android:gravity="center_vertical"
                                android:paddingLeft="@dimen/twenty"
                                android:paddingRight="@dimen/twenty"
                                android:text=""
                                android:textColor="@color/textcol" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="@dimen/twenty"
                                android:layout_marginLeft="@dimen/twenty"
                                android:background="@color/white"
                                android:fontFamily="@font/raleway"
                                android:gravity="center"
                                android:paddingLeft="@dimen/three"
                                android:paddingRight="@dimen/three"
                                android:text="Name"
                                android:textColor="@color/black"
                                android:textSize="@dimen/twelve" />


                        </RelativeLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/fifteen"
                        android:orientation="vertical"
                        android:paddingLeft="@dimen/fifteen"
                        android:paddingRight="@dimen/fifteen">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/email"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/fifty"
                                android:layout_marginTop="@dimen/ten"
                                android:background="@drawable/box"
                                android:gravity="center_vertical"
                                android:paddingLeft="@dimen/twenty"
                                android:paddingRight="@dimen/twenty"
                                android:text=""
                                android:textColor="@color/textcol" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="@dimen/twenty"
                                android:layout_marginLeft="@dimen/twenty"
                                android:background="@color/white"
                                android:fontFamily="@font/raleway"
                                android:gravity="center"
                                android:paddingLeft="@dimen/three"
                                android:paddingRight="@dimen/three"
                                android:text="Email"
                                android:textColor="@color/black"
                                android:textSize="@dimen/twelve" />

                        </RelativeLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/fifteen"
                        android:orientation="vertical"
                        android:paddingLeft="@dimen/fifteen"
                        android:paddingRight="@dimen/fifteen">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/phone"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/fifty"
                                android:layout_marginTop="@dimen/ten"
                                android:background="@drawable/box"
                                android:gravity="center_vertical"
                                android:paddingLeft="@dimen/twenty"
                                android:paddingRight="@dimen/twenty"
                                android:text="+91 "
                                android:textColor="@color/textcol" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="@dimen/twenty"
                                android:layout_marginLeft="@dimen/twenty"
                                android:background="@color/white"
                                android:fontFamily="@font/raleway"
                                android:gravity="center"
                                android:paddingLeft="@dimen/three"
                                android:paddingRight="@dimen/three"
                                android:text="Contact No."
                                android:textColor="@color/black"
                                android:textSize="@dimen/twelve" />

                        </RelativeLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/fifteen"
                        android:orientation="vertical"
                        android:paddingLeft="@dimen/fifteen"
                        android:paddingRight="@dimen/fifteen">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/city"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/fifty"
                                android:layout_marginTop="@dimen/ten"
                                android:background="@drawable/box"
                                android:gravity="center_vertical"
                                android:paddingLeft="@dimen/twenty"
                                android:paddingRight="@dimen/twenty"
                                android:text=""
                                android:textColor="@color/textcol" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="@dimen/twenty"
                                android:layout_marginLeft="@dimen/twenty"
                                android:background="@color/white"
                                android:fontFamily="@font/raleway"
                                android:gravity="center"
                                android:paddingLeft="@dimen/three"
                                android:paddingRight="@dimen/three"
                                android:text="City"
                                android:textColor="@color/black"
                                android:textSize="@dimen/twelve" />

                        </RelativeLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/fifteen"
                        android:orientation="vertical"
                        android:paddingLeft="@dimen/fifteen"
                        android:paddingRight="@dimen/fifteen">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/state"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/fifty"
                                android:layout_marginTop="@dimen/ten"
                                android:background="@drawable/box"
                                android:gravity="center_vertical"
                                android:paddingLeft="@dimen/twenty"
                                android:paddingRight="@dimen/twenty"
                                android:text=""
                                android:textColor="@color/textcol" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="@dimen/twenty"
                                android:layout_marginLeft="@dimen/twenty"
                                android:background="@color/white"
                                android:fontFamily="@font/raleway"
                                android:gravity="center"
                                android:paddingLeft="@dimen/three"
                                android:paddingRight="@dimen/three"
                                android:text="State"
                                android:textColor="@color/black"
                                android:textSize="@dimen/twelve" />

                        </RelativeLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/fifteen"
                        android:orientation="vertical"
                        android:paddingLeft="@dimen/fifteen"
                        android:paddingRight="@dimen/fifteen">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/country"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/fifty"
                                android:layout_marginTop="@dimen/ten"
                                android:background="@drawable/box"
                                android:gravity="center_vertical"
                                android:paddingLeft="@dimen/twenty"
                                android:paddingRight="@dimen/twenty"
                                android:text=""
                                android:textColor="@color/textcol" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="@dimen/twenty"
                                android:layout_marginLeft="@dimen/twenty"
                                android:background="@color/white"
                                android:fontFamily="@font/raleway"
                                android:gravity="center"
                                android:paddingLeft="@dimen/three"
                                android:paddingRight="@dimen/three"
                                android:text="Country"
                                android:textColor="@color/black"
                                android:textSize="@dimen/twelve" />

                        </RelativeLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/fifteen"
                        android:orientation="vertical"
                        android:paddingLeft="@dimen/fifteen"
                        android:paddingRight="@dimen/fifteen">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/pincode"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/fifty"
                                android:layout_marginTop="@dimen/ten"
                                android:background="@drawable/box"
                                android:gravity="center_vertical"
                                android:paddingLeft="@dimen/twenty"
                                android:paddingRight="@dimen/twenty"
                                android:text=""
                                android:textColor="@color/textcol" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="@dimen/twenty"
                                android:layout_marginLeft="@dimen/twenty"
                                android:background="@color/white"
                                android:fontFamily="@font/raleway"
                                android:gravity="center"
                                android:paddingLeft="@dimen/three"
                                android:paddingRight="@dimen/three"
                                android:text="Area"
                                android:textColor="@color/black"
                                android:textSize="@dimen/twelve" />

                        </RelativeLayout>

                    </LinearLayout>


                </LinearLayout>
            </androidx.core.widget.NestedScrollView>

        </RelativeLayout>

    </LinearLayout>

</FrameLayout>